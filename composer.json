{
    "name": "welabs/laravel-generator",
    "description": "WeLabs Laravel Generator",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mitul Golakiya",
            "email": "me@mitul.me"
        },
        {
            "name": "Wang Rui",
            "email": "wrmake@live.cn"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "~5.0",
        "prettus/l5-repository": "~2.1",
        "laracasts/flash": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "WeLabs\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
